}
<div class="row">
    <div class="col-md-2 col-xs-4 well">
        @using (Html.BeginForm("LogOff", "Account", new { area = "Clientes" }, FormMethod.Post, new { @id = "indexForm" }))
        {
            <ul class="nav nav-pills nav-stacked mnu-lateral">
                <li class="nav-header disabled"><a>Cadastro</a></li>
                <li><a href="@Url.Action("Index", "Inscricao")">Inscrição</a></li>
                <li><a href="@Url.Action("Edit", "Atleta")">Meus Dados</a></li>
                <li><a href="@Url.Action("Index", "Dependente")">Dependentes</a></li>
                <li><a href="@Url.Action("ForgotPassword", "Account")">Alterar Senha</a></li>
                <li class="nav-header disabled"><a>Consultas</a></li>
                @*<li><a href="#">Eventos</a></li>*@
