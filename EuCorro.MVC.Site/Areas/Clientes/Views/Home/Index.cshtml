                <li class="divider"></li>
                <li><a href="javascript:document.getElementById('logoutForm').submit()" title="Fazer Log Off"> Sair</a></li>
                @*<li><a href="@Url.Action("LogOff", "Account")">Sair</a></li>*@
            </ul>
        }
    </div>
    <div class="col-md-10 col-xs-12" id="conteudo">
        <div id="cabecalho-dados">
            <hgroup class="title text-center">
                <h2>EUCORRO Assessoria de Eventos Esportivos</h2>
            </hgroup>
            <br />            
            <div class="col-md-6 col-xs-12">
                <div class="ImagePreviewBox text-right">
                    <span>
                        <img src="~/img/Logo-perfil-EuCorro.jpg" class="img-thumbnail img-responsive" height="170" width="200" />
                    </span>
                </div>
            </div>
            <div class="col-md-6 col-xs-12">
                <div class="ImagePreviewBox">
                    <span>
                        <img src="~/img/Logo-topo.jpg" class="img-thumbnail img-responsive" height="170"  width="270" />
                    </span>
                </div>
            </div>
        </div>
        <div id="partial"></div>
